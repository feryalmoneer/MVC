@model Order

<form asp-controller="Order" asp-action="Details">
    <div class="form-group">
        <label asp-for="OrderName" class="control-label"></label>
        <input asp-for="OrderName" class="form-control" readonly />
    </div>

    <div class="form-group">
        <label asp-for="price" class="control-label"></label>
        <input asp-for="price" class="form-control" readonly />

    </div>

    <div class="form-group">
        <label asp-for="OrderDateCreated" class="control-label"></label>
        <input asp-for="OrderDateCreated" class="form-control" readonly />
    </div>

</form>
